extends layout-single

block content
  
  include oauth_links
  
  div(class="tn-col-1-1")
    a(href='/login', class=" tn-btn tn-element-color-main-red tn-element-color-onhover-main-red-dark3 btn-primary") I Have an account

  
  div(class="tn-separator")
    
  form(method="post", class="login-form")
    |Username
    input(type="text", name="username", class="tn-element-color-main-grey-light4")
    br
    |Password
    input(type="password", name="password", class="tn-element-color-main-grey-light4")
    br
    br
    input(type="submit", value="Register", class="tn-col-1-1 tn-btn tn-element-color-main-red tn-element-color-onhover-main-red-dark3 btn-primary tn-btn-disabled")
